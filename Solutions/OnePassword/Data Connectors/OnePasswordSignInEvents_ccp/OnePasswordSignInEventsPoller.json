[
    {
        "type": "Microsoft.SecurityInsights/dataConnectors",
        "apiVersion": "2022-10-01-preview",
        "name": "OnePasswordSignIn",
        "kind": "RestApiPoller",
        "properties": {
            "connectorDefinitionName": "OnePasswordSignIn",
            "dataType": "OnePasswordSignInEvents_CL",
            "dcrConfig": {
                "streamName": "Custom-OnePasswordSignInEvents"
            },
            "auth": {
                "authType": "APIKey",
                "APIKeyName": "Authorization",
                "APIKeyIdentifier": "Bearer "
            },
            "request": {
                "apiEndpoint": "{{BaseUr}}/api/v1/signinattempts",
                "rateLimitQPS": 10,
                "queryWindowInMin": 5,
                "httpMethod": "Post",
                "queryTimeFormat": "yyyy-MM-ddTHH:mm:ssZ",
                "retryCount": 2,
                "timeoutInSeconds": 60,
                "headers": {
                    "Content-Type": "application/json"
                },
                "queryParametersTemplate": "{\"limit\": 1000, \"start_time\": \"{_QueryWindowStartTime}\", \"end_time\": \"{_QueryWindowEndTime}\" }",
                "isPostPayloadJson": true
            },
            "response": {
                "eventsJsonPaths": [
                    "$.items"
                ],
                "format": "json"
            },
            "paging": {
                "pagingType": "NextPageToken",
                "nextPageParaName": "cursor",
                "nextPageTokenJsonPath": "$.cursor",
                "hasNextFlagJsonPath": "$.has_more"
            }
        }
    }
]
{
  "version": "2.0",
  "extensions": {
    "queues": {
      "maxPollingInterval": "00:01:00",
      "visibilityTimeout": "00:00:30",
      "batchSize": 1,
      "newBatchThreshold": 0,
      "maxDequeueCount": 3,
      "messageEncoding": "base64"
    }
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request" 
      }
    },
    "fileLoggingMode": "always",
    "logLevel": {
      "default": "Information", 
      "Host.Results": "Error",
      "Host": "Error",
      "Function": "Error",
      "Host.Aggregator": "Information"
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[3.*, 4.0.0)"
  },
  "managedDependency": {
    "enabled": true
  },
  "functionTimeout": "00:10:00"
}